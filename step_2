#include <iostream>
using namespace std;
int main(){
	setlocale(LC_CTYPE, "Russian");
	int a, s;
	cout << "Введите высоту : ";
	cin >> a;
	cout << "Введите ширину : ";
	cin >> s;
	for (int i = 0; i<s; i++)
	{
		for (int j = 0; j<a; j++)
		{
			if ((i >= 1 && i <= s - 2) && (j >= 1 && j <= a - 2))
				cout << " ";
			else
				cout << "*";
		}
		cout << endl;
	}
}
